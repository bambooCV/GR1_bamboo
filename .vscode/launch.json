        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
        },
        {
            "name": "Python: Accelerate Launch",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/traj_predict_policy_ddp.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "accelerate",
                "launch"
            ]
        }

